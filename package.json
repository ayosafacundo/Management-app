{
  "name": "management-app",
  "description": "Management App for UC Berkely, Idea originated from Sharpen.Design prompt",
  "author": "Facundo Ezequiel Ayosa",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "build": "cd .\\frontend\\ && npm run build",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client deploy": "cd .\\frontend\\ && npm run deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayosafacundo/management-app.git"
  },
  "bugs": {
    "url": "https://github.com/ayosafacundo/management-app/issues"
  },
  "keywords": [
    "Management App",
    "Uni Management App",
    "React.js",
    "Node.js",
    "Express.js",
    "mongoose",
    "MongoDB"
  ],
  "license": "ISC",
  "homepage": "",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.3.5"
  },
  "devDependencies": {
    "concurrently": "^7.2.1",
    "nodemon": "^2.0.16"
  }
}